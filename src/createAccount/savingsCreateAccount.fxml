<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="pane" stylesheets="@savingsCreateAccount.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="createAccount.SavingsCreateAccount">
   <center>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <Label text="Full Name">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="name" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <children>
                        <Label text="Date of birth">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <DatePicker fx:id="dob" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="200.0" />
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <Label text="Phone Number">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="phoneNumber" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <Label text="Residential Address Line 1">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="residentialAddress1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                     <children>
                        <Label text="Residential Address Line 2">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="residentialAddress2" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                     <children>
                        <Label text="Residential Address Line 3">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="residentialAddress3" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <children>
                        <Label text="email Address">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="emailAddress" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <Label text="Aadhar Number">
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font></Label>
                        <TextField fx:id="aadharNumber" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#submitOnAction" prefHeight="25.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                     <children>
                        <Button mnemonicParsing="false" onAction="#resetOnAction" prefWidth="200.0" text="Reset">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow />
                           </effect>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#submitOnAction" prefWidth="200.0" text="Submit">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="Comic Sans MS" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow />
                           </effect>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="20.0" left="300.0" right="300.0" top="20.0" />
         </BorderPane.margin>
         <effect>
            <DropShadow />
         </effect></AnchorPane>
   </center>
</BorderPane>
